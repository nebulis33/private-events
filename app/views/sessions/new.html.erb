<h1>Login</h1>
<%= form_tag sessions_path do |form| %>
    <div class="field">
        <%= label_tag :username %>
        <%= text_field_tag :username %>
  </div>
  <div class="field">
    <%= label_tag :email %>
    <%= text_field_tag :email %>
  </div>  
    <div class="actions">
    <%= submit_tag "Login" %>
  </div>
<% end %>